<div class="row list-wrapper">
  <ul class="list-group songs">
    {{#each model.quotes as |quoteList|}}
      <li class="list-group-item">
        <em>
          {{#each quoteList as |quoteLine|}}
            <strong>{{quoteLine.character}}</strong>{{#if quoteLine.character}}: {{/if}}
            {{quoteLine.quote}}<br/>
          {{/each}}
        </em>
      </li>
    {{/each}}
  </ul>
</div>