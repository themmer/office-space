<div class="row list-wrapper">
  <ul class="list-group songs">
    {{#each model.quotes as |quoteList|}}
      <li class="list-group-item">
        <em>
          {{#each quoteList as |quoteLine|}}
            {{line-quote selectedCharacter=model quoteLine=quoteLine}}
          {{/each}}
        </em>
      </li>
    {{/each}}
  </ul>
</div>